		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>4</length>
		<editable>true</editable>
		<editable>false</editable>
	</field>

	<field>
		<name>dc</name>
		<!-- Dígitos de Control de la cuenta bancaria del cliente -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Dígitos de Control")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>2</length>
		<calculated>true</calculated>
		<editable>false</editable>
	</field>

	<field>
		<name>cuenta</name>
		<!-- Número de la cuenta bancaria del cliente -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Nº Cuenta")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>
		<editable>true</editable>
		<editable>false</editable>
	</field>
	
