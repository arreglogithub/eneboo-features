		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>4</length>
		<editable>true</editable>
		<editable>false</editable>
	</field>

	<field>
		<name>ctaagencia</name>
		<!-- Oficina de la cuenta bancaria del cliente -->
